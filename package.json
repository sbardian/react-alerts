    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-dom": "3.2.2",
    "prettier": "1.17.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-testing-library": "7.0.1",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
