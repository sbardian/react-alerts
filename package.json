{
  "name": "react-alerts-plus",
  "version": "0.0.3",
  "description": "Alerts for react.",
  "author": "Brian Andrews <sbardian@gmail.com> (https://github.com/sbardian)",
  "license": "MIT",
  "keywords": [
    "alert",
    "alerts",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sbardian/react-alerts-plus"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "dev": "webpack && webpack-dev-server  --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
